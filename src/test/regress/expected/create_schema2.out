declare
    var_name text;
    query_str text;
begin
    select rolname into var_name from pg_authid where oid=10;

    query_str := 'create schema "' || var_name || '"';
    EXECUTE IMMEDIATE query_str;
    
    query_str := 'drop schema "' || var_name ||'" CASCADE';
    EXECUTE IMMEDIATE query_str;
end;

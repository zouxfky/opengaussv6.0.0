msgid "  -P, --function=NAME(args)             restore named function(s)\n"
msgstr "  -P, --function=NAME(args)               恢复指定函数\n"

#: pg_restore.cpp:771
#, c-format
msgid "  -s, --schema-only                     restore only the schema, no data\n"
msgstr "  -s, --schema-only                       只恢复模式, 不恢复数据\n"

#: pg_restore.cpp:772
#, c-format
msgid "  -S, --sysadmin=NAME                   system admin user name to use for disabling triggers\n"
msgstr "  -S, --sysadmin=NAME                     用于禁用触发器的系统管理员用户名\n"

#: pg_restore.cpp:773
#, c-format
msgid "  -t, --table=NAME                      restore named table(s)\n"
msgstr "  -t, --table=NAME                        恢复指定的表\n"

#: pg_restore.cpp:774
#, c-format
msgid "  -T, --trigger=NAME                    restore named trigger(s)\n"
msgstr "  -T, --trigger=NAME                      恢复指定触发器\n"

#: pg_restore.cpp:775
#, c-format
msgid "  -x, --no-privileges/--no-acl          skip restoration of access privileges (grant/revoke)\n"
msgstr "  -x, --no-privileges/--no-acl            skip恢复访问权限(grant/revoke)\n"

#: pg_restore.cpp:776
#, c-format
msgid "  -1, --single-transaction              restore as a single transaction\n"
msgstr "  -1, --single-transaction                恢复为单个事务\n"

#: pg_restore.cpp:777
#, c-format
msgid "  --disable-triggers                    disable triggers during data-only restore\n"
msgstr "  --disable-triggers                      在仅数据恢复期间禁用触发器\n"

#: pg_restore.cpp:778
#, c-format
msgid ""
"  --no-data-for-failed-tables           do not restore data of tables that could not be\n"
"                                        created\n"
msgstr ""
"  --no-data-for-failed-tables           对那些无法创建的表不进行数据恢复\n"
"\n"

#: pg_restore.cpp:781
#, c-format
msgid "  --no-publications                     do not restore publications\n"
msgstr "  --no-publications                       不恢复出版物\n"

#: pg_restore.cpp:783
#, c-format
msgid "  --no-security-labels                  do not restore security labels\n"
msgstr "  --no-security-labels                    不恢复安全标签\n"

#: pg_restore.cpp:785
#, c-format
msgid "  --no-subscriptions                    do not restore subscriptions\n"
msgstr "  --no-subscriptions                      不恢复订阅\n"

#: pg_restore.cpp:787
#, c-format
msgid "  --no-tablespaces                      do not restore tablespace assignments\n"
msgstr "  --no-tablespaces                        不恢复表空间分配\n"

#: pg_restore.cpp:788
#, c-format
msgid "  --section=SECTION                     restore named section (pre-data, data, or post-data)\n"
msgstr "  --section=SECTION                       恢复指定的section(前数据, 数据或后数据)\n"

#: pg_restore.cpp:789
#, c-format
msgid ""
"  --use-set-session-authorization       use SET SESSION AUTHORIZATION commands instead of\n"
"                                        ALTER OWNER commands to set ownership\n"
msgstr ""
"  --use-set-session-authorization       使用 SESSION AUTHORIZATION 命令代替\n"
"                                        ALTER OWNER 命令来设置所有权\n"

#: pg_restore.cpp:791
#, c-format
msgid ""
"  --pipeline                            use pipeline to pass the password,\n"
"                                        forbidden to use in terminal\n"
msgstr ""
"  --pipeline                            使用管道传递密码，禁止在终端使用\n"
"\n"

#: pg_restore.cpp:795
#, c-format
msgid "  -h, --host=HOSTNAME                   database server host or socket directory\n"
msgstr "  -h, --host=HOSTNAME                    数据库服务器的主机或套接字目录\n"

#: pg_restore.cpp:796
#, c-format
msgid "  -p, --port=PORT                       database server port number\n"
msgstr "  -p, --port=PORT                        数据库服务器端口号\n"

#: pg_restore.cpp:797
#, c-format
msgid "  -U, --username=NAME                   connect as specified database user\n"
msgstr "  -U, --username=NAME                    连接指定的数据库用户\n"

#: pg_restore.cpp:798
#, c-format
msgid "  -w, --no-password                     never prompt for password\n"
msgstr "  -w, --no-password                      从不提示输入密码\n"

#: pg_restore.cpp:799
#, c-format
msgid "  -W, --password=PASSWORD               the password of specified database user\n"
msgstr "  -W, --password=PASSWORD                指定数据库用户的密码\n"

#: pg_restore.cpp:800
#, c-format
msgid "  --role=ROLENAME                       do SET ROLE before restore\n"
msgstr "  --role=ROLENAME                        在恢复前设置角色\n"

#: pg_restore.cpp:801
#, c-format
msgid "  --rolepassword=ROLEPASSWORD           the password for role\n"
msgstr "  --rolepassword=ROLEPASSWORD             角色密码\n"
